{
  "name": "base_app",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android:default": "react-native run-android",
    "android": "react-native run-android --variant devDebug",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "build": "cd android && ./gradlew assembleRelease && cd ../",
    "build:ios": "react-native bundle --platform ios --entry-file index.js --bundle-output ./bundles/main.jsbundle --assets-dest  ./bundles --dev false",
    "build:beta": "cd android&&./gradlew assembleBetaRelease&&cd ../",
    "build:pro": "cd android&& cross-env ENVFILE=.env.production ./gradlew assembleProRelease&&cd ../",
    "test": "jest",
    "icon": "npx iconfont-rn",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx"
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.12.1",
    "@react-navigation/bottom-tabs": "^6.0.9",
    "@react-navigation/drawer": "^6.1.8",
    "@react-navigation/native": "^6.0.6",
    "@react-navigation/native-stack": "^6.2.5",
    "@react-navigation/stack": "^6.0.11",
    "@rematch/core": "^2.2.0",
    "@rematch/loading": "^2.1.2",
    "@rematch/persist": "^2.1.2",
    "@rematch/select": "^3.1.2",
    "@types/redux-logger": "^3.0.9",
    "axios": "^0.24.0",
    "jsencrypt": "^3.2.1",
    "native-base": "^3.2.2",
    "react": "17.0.2",
    "react-hook-form": "^7.27.0",
    "react-native": "0.66.4",
    "react-native-elements": "^3.4.2",
    "react-native-gesture-handler": "^2.1.0",
    "react-native-reanimated": "^2.3.1",
    "react-native-root-siblings": "^4.1.1",
    "react-native-root-toast-tips": "^1.1.4",
    "react-native-safe-area-context": "^3.3.2",
    "react-native-screens": "^3.10.1",
    "react-native-splash-screen": "^3.3.0",
    "react-native-svg": "^12.1.1",
    "react-native-vector-icons": "^9.1.0",
    "react-redux": "^7.2.6",
    "redux-logger": "^3.0.6",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/jest": "^26.0.23",
    "@types/react-native": "^0.66.4",
    "@types/react-test-renderer": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "babel-jest": "^26.6.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^7.14.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.66.2",
    "react-native-config": "^1.4.5",
    "react-native-iconfont-cli": "^2.2.4",
    "react-test-renderer": "17.0.2",
    "typescript": "^4.4.4"
  },
  "resolutions": {
    "@types/react": "^17"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
